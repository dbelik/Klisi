{
    "Enviropment": "Development",
    "Logger": {
        "Combined": "../logs/klisi.log",
        "Error": "../logs/klisi-error.log"
    },
    "Server": {
        "Port": 443,
        "Hostname": "localhost"
    },
    "SSL": {
        "KeyPath": "../ssl/key.pem",
        "CertPath": "../ssl/cert.pem",
        "Passphrase": "your_passphrase"
    },
    "Auth": {
        "Google": {
            "ID": "google_id",
            "Secret": "google_secret"
        },
        "Cookie": {
            "MaxAge": 3600000,
            "Keys": ["your_cookie_key"]
        }
    },
    "Database": {
        "URL": "your_url"
    }
}